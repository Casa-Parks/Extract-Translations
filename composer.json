{
    "name": "casa-parks/extract-translations",
    "description": "Casa-Parks' Laravel translation extracter.",
    "keywords": ["extract", "translation", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Connor S. Parks",
            "email": "connor@connorvg.tv"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/contracts": "^5.4",
        "illuminate/support": "^5.4",
        "illuminate/translation": "^5.4"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "CasaParks\\ExtractTranslations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
